<background>
	<starttime>
		<year>2021</year>
		<month>10</month>
		<day>01</day>
		<hour>0</hour>
		<minute>00</minute>
		<second>00</second>
	</starttime>
	<!-- This animation will start at midnight. -->
	<static>
		<file>/usr/share/backgrounds/BigSur/Night.jpg</file>
		<duration>18000.0</duration>
	</static>

<transition type="overlay">
<duration>5400.0</duration>
<from>/usr/share/backgrounds/BigSur/Night.jpg</from>
<to>/usr/share/backgrounds/BigSur/Dawn.jpg</to>
</transition>

	<static>
		<file>/usr/share/backgrounds/BigSur/Daybreak.jpg</file>
		<duration>7200.0</duration>
	</static>

<transition type="overlay">
<duration>5400.0</duration>
<from>/usr/share/backgrounds/BigSur/Daybreak.jpg</from>
<to>/usr/share/backgrounds/BigSur/Morning-Early.jpg</to>
</transition>

	<static>
		<file>/usr/share/backgrounds/BigSur/Morning-Late.jpg</file>
		<duration>5400.0</duration>
	</static>

<transition type="overlay">
<duration>16200.0</duration>
<from>/usr/share/backgrounds/BigSur/Morning-Late.jpg</from>
<to>/usr/share/backgrounds/BigSur/Afternoon.jpg</to>
</transition>

	<static>
		<file>/usr/share/backgrounds/BigSur/Dusk.jpg</file>
		<duration>5400.0</duration>
	</static>

<transition type="overlay">
<duration>5400.0</duration>
<from>/usr/share/backgrounds/BigSur/Dusk.jpg</from>
<to>/usr/share/backgrounds/BigSur/Evening.jpg</to>
</transition>

<static>
		<file>/usr/share/backgrounds/BigSur/Night.jpg</file>
		<duration>18000.0</duration>
	</static>
	
</background>
